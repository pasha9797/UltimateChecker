<UserControl x:Class="UltimateChecker.Classes.Checkers.White.WhiteCheckerUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UltimateChecker.Classes.Checkers.White"
             mc:Ignorable="d" 
             d:DesignHeight="85" d:DesignWidth="85">
    <Grid>
        <Ellipse Fill="#fff" HorizontalAlignment="Left" Height="85" Stroke="Black" VerticalAlignment="Top" Width="85" MouseDown="Ellipse_MouseDown" MouseMove="Ellipse_MouseMove" MouseUp="Ellipse_MouseUp"/>

    </Grid>
</UserControl>
